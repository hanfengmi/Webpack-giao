{
    "presets": [
        [
        "@babel/preset-env",
        {
            "targets": {
                "browsers": ["> 1%", "last 2 versions"]
            },
            "corejs": 2,
            "useBuiltIns": "usage"
        }]
    ],
    "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
          }
        ]
    ]
}